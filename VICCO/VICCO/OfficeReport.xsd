                    <CommandText>SELECT MATERIAL_GROUP, FORMAT(CAST(@TO_DATE AS DATE), 'MMM yy') AS MONTH_NAME, (SELECT SUM(CAST(ISNULL(POP.AMOUNT, 0) + ISNULL(POP.AMOUNT, 0) * (ISNULL(MM.CGST, 0) + ISNULL(MM.SGST, 0)) / 100 AS DECIMAL(18 , 2))) AS Expr1 FROM PURCHASE_ORDER_PRODUCT AS POP INNER JOIN MATERIAL AS MM ON MM.MATERIAL_ID = POP.PRODUCT_ID WHERE (POP.PURCHASEORDER_ID IN (SELECT PURCHASEORDER_ID FROM PURCHASE_ORDER WHERE (DATE BETWEEN '1 APR ' + CAST(YEAR(DATEADD(YEAR, - 1, @TO_DATE)) AS VARCHAR) AND '31 MAR ' + CAST(YEAR(@TO_DATE) AS VARCHAR)))) AND (POP.PRODUCT_ID IN (SELECT MATERIAL_ID FROM MATERIAL WHERE (MATERIAL_GROUP = M.MATERIAL_GROUP)))) AS PREVIOUS_YEAR_SALE, (SELECT SUM(CAST(ISNULL(POP.AMOUNT, 0) + ISNULL(POP.AMOUNT, 0) * (ISNULL(MM.CGST, 0) + ISNULL(MM.SGST, 0)) / 100 AS DECIMAL(18 , 2))) AS Expr1 FROM PURCHASE_ORDER_PRODUCT AS POP INNER JOIN MATERIAL AS MM ON MM.MATERIAL_ID = POP.PRODUCT_ID WHERE (POP.PURCHASEORDER_ID IN (SELECT PURCHASEORDER_ID FROM PURCHASE_ORDER AS PURCHASE_ORDER_8 WHERE (DATE BETWEEN '1 APR ' + CAST(YEAR(DATEADD(YEAR, - 1, @TO_DATE)) AS VARCHAR) AND '31 MAR ' + CAST(YEAR(@TO_DATE) AS VARCHAR)))) AND (POP.PRODUCT_ID IN (SELECT MATERIAL_ID FROM MATERIAL AS MATERIAL_8 WHERE (MATERIAL_GROUP = M.MATERIAL_GROUP)))) / 12 AS PREVIOUS_YEAR_AVG_SALE, (SELECT SUM(CAST(ISNULL(POP.AMOUNT, 0) + ISNULL(POP.AMOUNT, 0) * (ISNULL(MM.CGST, 0) + ISNULL(MM.SGST, 0)) / 100 AS DECIMAL(18 , 2))) AS Expr1 FROM PURCHASE_ORDER_PRODUCT AS POP INNER JOIN MATERIAL AS MM ON MM.MATERIAL_ID = POP.PRODUCT_ID WHERE (POP.PURCHASEORDER_ID IN (SELECT PURCHASEORDER_ID FROM PURCHASE_ORDER AS PURCHASE_ORDER_7 WHERE (DATE BETWEEN @TO_DATE AND DATEADD(DAY, - 1, DATEADD(MONTH, 1, @TO_DATE))))) AND (POP.PRODUCT_ID IN (SELECT MATERIAL_ID FROM MATERIAL AS MATERIAL_7 WHERE (MATERIAL_GROUP = M.MATERIAL_GROUP)))) AS SELECTED_MONTH_VALUE, (SELECT SUM(CAST(ISNULL(POP.AMOUNT, 0) + ISNULL(POP.AMOUNT, 0) * (ISNULL(MM.CGST, 0) + ISNULL(MM.SGST, 0)) / 100 AS DECIMAL(18 , 2))) AS Expr1 FROM PURCHASE_ORDER_PRODUCT AS POP INNER JOIN MATERIAL AS MM ON MM.MATERIAL_ID = POP.PRODUCT_ID WHERE (POP.PURCHASEORDER_ID IN (SELECT PURCHASEORDER_ID FROM PURCHASE_ORDER AS PURCHASE_ORDER_6 WHERE (DATE BETWEEN CASE WHEN DATEDIFF(MONTH , @FROM_DATE , @TO_DATE) &lt; 0 THEN DATEADD(YEAR , - 1 , @FROM_DATE) ELSE @FROM_DATE END AND DATEADD(DAY, - 1, DATEADD(MONTH, 1, @TO_DATE))))) AND (POP.PRODUCT_ID IN (SELECT MATERIAL_ID FROM MATERIAL AS MATERIAL_6 WHERE (MATERIAL_GROUP = M.MATERIAL_GROUP)))) AS SELECTED_MONTH_CUMMULATIVE, ISNULL((SELECT SUM(CAST(ISNULL(POP.AMOUNT, 0) + ISNULL(POP.AMOUNT, 0) * (ISNULL(MM.CGST, 0) + ISNULL(MM.SGST, 0)) / 100 AS DECIMAL(18 , 2))) AS Expr1 FROM PURCHASE_ORDER_PRODUCT AS POP INNER JOIN MATERIAL AS MM ON MM.MATERIAL_ID = POP.PRODUCT_ID WHERE (POP.PURCHASEORDER_ID IN (SELECT PURCHASEORDER_ID FROM PURCHASE_ORDER AS PURCHASE_ORDER_5 WHERE (DATE BETWEEN @FROM_DATE AND DATEADD(DAY, - 1, DATEADD(MONTH, 1, @TO_DATE))))) AND (POP.PRODUCT_ID IN (SELECT MATERIAL_ID FROM MATERIAL AS MATERIAL_5 WHERE (MATERIAL_GROUP = M.MATERIAL_GROUP)))) / (CASE WHEN DATEDIFF(MONTH , @FROM_DATE , @TO_DATE) &lt; 0 THEN DATEDIFF(MONTH , DATEADD(YEAR , - 1 , @FROM_DATE) , @TO_DATE) ELSE DATEDIFF(MONTH , @FROM_DATE , @TO_DATE) END + 1), 0) AS CURRENT_YEAR_AVG, ISNULL((SELECT SUM(CAST(ISNULL(POP.AMOUNT, 0) + ISNULL(POP.AMOUNT, 0) * (ISNULL(MM.CGST, 0) + ISNULL(MM.SGST, 0)) / 100 AS DECIMAL(18 , 2))) AS Expr1 FROM PURCHASE_ORDER_PRODUCT AS POP INNER JOIN MATERIAL AS MM ON MM.MATERIAL_ID = POP.PRODUCT_ID WHERE (POP.PURCHASEORDER_ID IN (SELECT PURCHASEORDER_ID FROM PURCHASE_ORDER AS PURCHASE_ORDER_4 WHERE (DATE BETWEEN @FROM_DATE AND DATEADD(DAY, - 1, DATEADD(MONTH, 1, @TO_DATE))))) AND (POP.PRODUCT_ID IN (SELECT MATERIAL_ID FROM MATERIAL AS MATERIAL_4 WHERE (MATERIAL_GROUP = M.MATERIAL_GROUP)))) / (CASE WHEN DATEDIFF(MONTH , @FROM_DATE , @TO_DATE) &lt; 0 THEN DATEDIFF(MONTH , DATEADD(YEAR , - 1 , @FROM_DATE) , @TO_DATE) ELSE DATEDIFF(MONTH , @FROM_DATE , @TO_DATE) END + 1), 0) - ISNULL((SELECT SUM(CAST(ISNULL(POP.AMOUNT, 0) + ISNULL(POP.AMOUNT, 0) * (ISNULL(MM.CGST, 0) + ISNULL(MM.SGST, 0)) / 100 AS DECIMAL(18 , 2))) AS Expr1 FROM PURCHASE_ORDER_PRODUCT AS POP INNER JOIN MATERIAL AS MM ON MM.MATERIAL_ID = POP.PRODUCT_ID WHERE (POP.PURCHASEORDER_ID IN (SELECT PURCHASEORDER_ID FROM PURCHASE_ORDER AS PURCHASE_ORDER_3 WHERE (DATE BETWEEN '1 APR ' + CAST(YEAR(DATEADD(YEAR, - 1, @TO_DATE)) AS VARCHAR) AND '31 MAR ' + CAST(YEAR(@TO_DATE) AS VARCHAR)))) AND (POP.PRODUCT_ID IN (SELECT MATERIAL_ID FROM MATERIAL AS MATERIAL_3 WHERE (MATERIAL_GROUP = M.MATERIAL_GROUP)))) / 12, 0) AS BALANCE, ((SELECT SUM(CAST(ISNULL(POP.AMOUNT, 0) + ISNULL(POP.AMOUNT, 0) * (ISNULL(MM.CGST, 0) + ISNULL(MM.SGST, 0)) / 100 AS DECIMAL(18 , 2))) AS Expr1 FROM PURCHASE_ORDER_PRODUCT AS POP INNER JOIN MATERIAL AS MM ON MM.MATERIAL_ID = POP.PRODUCT_ID WHERE (POP.PURCHASEORDER_ID IN (SELECT PURCHASEORDER_ID FROM PURCHASE_ORDER AS PURCHASE_ORDER_2 WHERE (DATE BETWEEN @FROM_DATE AND DATEADD(DAY, - 1, DATEADD(MONTH, 1, @TO_DATE))))) AND (POP.PRODUCT_ID IN (SELECT MATERIAL_ID FROM MATERIAL AS MATERIAL_2 WHERE (MATERIAL_GROUP = M.MATERIAL_GROUP)))) / (CASE WHEN DATEDIFF(MONTH , @FROM_DATE , @TO_DATE) &lt; 0 THEN DATEDIFF(MONTH , DATEADD(YEAR , - 1 , @FROM_DATE) , @TO_DATE) ELSE DATEDIFF(MONTH , @FROM_DATE , @TO_DATE) END + 1)) / ((SELECT SUM(CAST(ISNULL(POP.AMOUNT, 0) + ISNULL(POP.AMOUNT, 0) * (ISNULL(MM.CGST, 0) + ISNULL(MM.SGST, 0)) / 100 AS DECIMAL(18 , 2))) AS Expr1 FROM PURCHASE_ORDER_PRODUCT AS POP INNER JOIN MATERIAL AS MM ON MM.MATERIAL_ID = POP.PRODUCT_ID WHERE (POP.PURCHASEORDER_ID IN (SELECT PURCHASEORDER_ID FROM PURCHASE_ORDER AS PURCHASE_ORDER_1 WHERE (DATE BETWEEN '1 APR ' + CAST(YEAR(DATEADD(YEAR, - 1, @TO_DATE)) AS VARCHAR) AND '31 MAR ' + CAST(YEAR(@TO_DATE) AS VARCHAR)))) AND (POP.PRODUCT_ID IN (SELECT MATERIAL_ID FROM MATERIAL AS MATERIAL_1 WHERE (MATERIAL_GROUP = M.MATERIAL_GROUP)))) / 12) * 100 - 100 AS PERCENTAGE FROM MATERIAL AS M GROUP BY MATERIAL_GROUP ORDER BY MATERIAL_GROUP</CommandText>
