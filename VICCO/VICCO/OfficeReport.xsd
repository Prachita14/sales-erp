                </InsertCommand>
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT R.REGION_NAME,'' AS MONTH_NAME,
		ISNULL((SELECT SUM(GRAND_TOTAL) FROM PURCHASE_ORDER PO JOIN DISTRIBUTOR D ON D.DISTRIBUTOR_ID=PO.CUSTOMER_ID
		JOIN PURCHASE_ORDER_PRODUCT POP ON POP.PURCHASEORDER_ID=PO.PURCHASEORDER_ID
		WHERE REGION=R.REGION_NAME AND DATE BETWEEN DATEADD(YEAR,-1,@FROM_DATE) AND (@TO_DATE) 
		AND POP.PRODUCT_ID=1),0) AS PREVIOUS_YEAR_VALUE,

		ISNULL((SELECT SUM(GRAND_TOTAL) FROM PURCHASE_ORDER PO JOIN DISTRIBUTOR D ON D.DISTRIBUTOR_ID=PO.CUSTOMER_ID
		JOIN PURCHASE_ORDER_PRODUCT POP ON POP.PURCHASEORDER_ID=PO.PURCHASEORDER_ID
		WHERE REGION=R.REGION_NAME AND DATE BETWEEN DATEADD(YEAR,-1,@FROM_DATE) AND (@TO_DATE)
		AND POP.PRODUCT_ID=1)/12,0) AS PREVIOUS_YEAR_AVG_VALUE,

		ISNULL((SELECT SUM(GRAND_TOTAL) FROM PURCHASE_ORDER PO JOIN DISTRIBUTOR D ON D.DISTRIBUTOR_ID=PO.CUSTOMER_ID
		JOIN PURCHASE_ORDER_PRODUCT POP ON POP.PURCHASEORDER_ID=PO.PURCHASEORDER_ID
